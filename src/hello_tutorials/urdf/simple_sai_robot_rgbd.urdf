    <link name="us_front_right_link">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.01 0.02 0.04"/>
            </geometry>
            <material name="red" />
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.01 0.02 0.04"/>
            </geometry>
            <material name="red" />
        </collision>
        <inertial>
            <mass value="0.05"/>
            <inertia ixx="0.000007" ixy="0" ixz="0"
